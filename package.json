{
  "name": "graphql-joda-types",
  "version": "1.0.0",
  "scripts": {
    "prepack": "yarn run build:babel && yarn run build:flow",
    "build:babel": "babel src -d lib",
    "build:flow": "flow-copy-source src lib",
    "precommit": "pretty-quick --staged",
    "prepush": "eslint .",
    "test": "flow check && eslint . && jest"
  },
  "description": "js-joda types for GraphQL",
  "main": "lib/index.js",
  "repository": "https://github.com/meetup/graphql-joda-types",
  "author": "mike castleman <mlc@meetup.com>",
  "license": "MIT",
  "dependencies": {
    "js-joda": "^1.8.2"
  },
  "peerDependencies": {
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "flow-bin": "0.75.0",
    "flow-copy-source": "^2.0.0",
    "graphql": "0.13.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "prettier": "^1.13.6",
    "pretty-quick": "^1.6.0"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/lib/"
    ]
  }
}
